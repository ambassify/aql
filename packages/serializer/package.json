{
  "name": "@ambassify/aql-serializer",
  "version": "0.2.0",
  "description": "AQL serializer",
  "main": "src/index.js",
  "browser": "lib/index.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "preversion": "npm run test",
