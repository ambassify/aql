    "url": "https://github.com/ambassify/aql/issues"
  },
  "homepage": "https://github.com/ambassify/aql#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "eslint": "^5.16.0",
    "eslint-config-ambassify": "^1.2.0",
    "mocha": "^6.1.2",
    "npm-build-tools": "^2.2.5",
    "nyc": "^13.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
